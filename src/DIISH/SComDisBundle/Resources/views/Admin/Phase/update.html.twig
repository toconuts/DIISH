{% extends 'DIISHSComDisBundle::layout.html.twig' %}

{% block page_title 'Syndromic Surveillance - Phase - SComDis' %}
{% block product_name 'SComDis' %}
{% block content_title 'Syndromic Surveillance - Phase - Confirmation' %}

{% set menu = 'SComDis' %}
{% set sidebar_menu = 'scomdis_admin_phase' %}

{% block content %}
<table class="table table-striped table-bordered">
  <tbody>
    <tr>
      <th>ID</th>
      <td>{{ phase.id }}</td>
    </tr>
    <tr>
      <th>Threshold</th>
      <td>{{ phase.threshold }}</td>
    </tr>
    <tr>
      <th>Color</th>
      <td>{{ phase.color }}</td>
    </tr>
  </tbody>
</table>
<div class="form-actions">
  <a href="{{ pre_url }}" class="btn">&laquo; Back</a>
</div>
{% endblock %}
