{% extends 'DIISHSComDisBundle::layout.html.twig' %}

{% block page_title 'Syndromic Surveillance - PMH - SComDis' %}
{% block product_name 'SComDis' %}
{% block content_title 'Syndromic Surveillance - PMH - Edit' %}

{% set menu = 'SComDis' %}
{% set sidebar_menu = 'scomdis_admin_pmh' %}

{% block content %}
{{ form_start(form, {'attr': {'class': 'form-horizontal'}}) }}
  {{ form_errors(form) }}
    <div class="control-group {% if form.id.vars.errors|length > 0 %}error{% endif %}">
      <label class="control-label">ID</label>
      <div class="controls">
        {{ form_errors(form.id) }}
        {{ form_widget(form.id) }}
      </div>
    </div>
    <div class="control-group {% if form.clinic.vars.errors|length > 0 %}error{% endif %}">
      <label class="control-label">Clinic</label>
      <div class="controls">
        {{ form_errors(form.clinic) }}
        {{ form_widget(form.clinic) }}
      </div>
    </div>
    <div class="form-actions">
      <a href=javascript:history:back() class="btn">&laquo; Cancel</a>
      <input type="submit" class="btn btn-primary" value="Confirm">
    </div>
{{ form_end(form) }}
{% endblock %}
